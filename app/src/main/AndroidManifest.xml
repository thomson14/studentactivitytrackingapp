<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.studentactivitytrackingapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StudyTimerActivity">
        </activity>
        <activity
            android:name=".AddNoteActivity"
            android:parentActivityName=".NoteTakingActivity" />
        <activity
            android:name=".NoteTakingActivity"
            android:launchMode="singleTop" />
        <activity android:name=".ToDoList" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Register_Activity" />
        <activity android:name=".Login_Activity" />
        <activity android:name=".Home" />
        <activity android:name=".Log_and_Regis">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>